<Window x:Class="MythWeaverSheetReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Myth-Weavers Login" Height="132" Width="265" ResizeMode="CanMinimize">
    <Grid>
        <Button Content="Submit" Height="29" HorizontalAlignment="Left" Margin="175,70,0,0" Name="submit" VerticalAlignment="Top" Width="78" Click="submit_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="72,9,0,0" Name="usernameBox" VerticalAlignment="Top" Width="181" />
        <TextBlock Height="25" HorizontalAlignment="Left" Margin="8,11,0,0" Name="label1" Text="Username:" VerticalAlignment="Top" Width="74" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="72,41,0,0" Name="passwordBox" VerticalAlignment="Top" Width="181" />
        <TextBlock Height="25" HorizontalAlignment="Left" Margin="11,43,0,0" Name="label2" Text="Password:" VerticalAlignment="Top" Width="74" />
    </Grid>
</Window>
